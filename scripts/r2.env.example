# Cloudflare R2 endpoint (S3 compatible)
# Example: https://<account-id>.r2.cloudflarestorage.com
R2_ENDPOINT=

# Target bucket name
R2_BUCKET=

# R2 API token credentials
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Optional defaults
DATASET_VERSION=v1
# AWS region for R2 with aws cli (auto works for R2)
AWS_REGION=auto
